msgid "Create poll"
msgstr "Umfrage erstellen"

#: dudel/templates/index.html:7
msgid "Create a poll"
msgstr "Eine Umfrage erstellen"

#: dudel/templates/index.html:46
msgid "Recent polls"
msgstr "Neuste Umfragen"

#: dudel/templates/index.html:58
msgid "There are currently no public polls."
msgstr "Es gibt zurzeit keine öffentlichen Umfragen."

#: dudel/templates/index.html:63
msgid "My polls"
msgstr "Eigene Umfragen"

#: dudel/templates/index.html:74
msgid "You do not own any polls."
msgstr "Du hast keine eigenen Umfragen."

#: dudel/templates/index.html:79
msgid "Dudel statistics"
msgstr "Statistik"

#: dudel/templates/index.html:82
msgid "Polls created"
msgstr "Erstellte Umfragen"

#: dudel/templates/index.html:83
msgid "Votes cast"
msgstr "Abgegebene Stimmen"

#: dudel/templates/index.html:84
msgid "Users"
msgstr "Benutzer"

